{
	"manifest_version": 2,
	"name": "Emoji to English",
	"version": "2.1",
	"description": "Translates emojis to their Unicode names",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["names-dict.js", "emoji-replacer.js", "dom-manipulator.js"],
			"css": ["emoji-to-english.css"]
		}
	],
	"background": {
		"scripts": ["context-menu-handler.js"]
	},
	"applications": {
		"gecko": {
			"id": "emojisToEnglish@szupie"
		}
	},
	"options_ui": {
		"page": "settings.html"
	},
	"permissions": [
		"storage",
		"contextMenus"
	],
	"default_locale": "en"
}
